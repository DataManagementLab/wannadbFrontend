{
	"name": "wannadbfrontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"serve": "vite",
		"start": "git pull && npm install && npm run backend:check && npm run backend:create && vite --open",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"format": "prettier --write ./src",
		"format:check": "prettier ./src --check",
		"test": "vitest run --coverage",
		"test:watch": "vitest",
		"prepare": "husky install",
		"generate": "python scripts/generate.py",
		"g": "python scripts/generate.py",
		"todo": "python scripts/todo.py",
		"comment:check": "python scripts/check_comment.py",
		"backend:check": "python scripts/check_backend.py",
		"backend:create": "npm run backend:create:tables",
		"backend:create:tables": "python scripts/create_table.py"
	},
	"dependencies": {
		"axios": "^1.6.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.20.0"
	},
	"devDependencies": {
		"@testing-library/react": "^14.1.2",
		"@types/jest": "^29.5.10",
		"@types/react": "^18.2.37",
		"@types/react-dom": "^18.2.15",
		"@typescript-eslint/eslint-plugin": "^6.10.0",
		"@typescript-eslint/parser": "^6.10.0",
		"@vitejs/plugin-react": "^4.2.0",
		"@vitest/coverage-v8": "^0.34.6",
		"@vitest/ui": "^0.34.6",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.4",
		"husky": "^8.0.0",
		"jest": "^29.7.0",
		"jsdom": "^23.0.0",
		"prettier": "3.1.0",
		"sass": "^1.69.5",
		"ts-jest": "^29.1.1",
		"typescript": "^5.2.2",
		"vite": "^5.0.0",
		"vitest": "^0.34.6"
	}
}
